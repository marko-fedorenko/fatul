{
  "name": "gsc-fresh-analytics",
  "version": "1.0.0",
  "description": "GSC Fresh Analytics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node --watch server.js",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  },
  "engines": {
    "node": "18.x"
  }
}
